       <script type="text/javascript">
      var cookieValue = document.getElementById('demo');
      var x = parseInt(cookieValue.getAttribute('target'));
      var a2 = document.getElementById('demo2');
      var xx = parseInt(a2.getAttribute('target'));
      //var cookieValue2 = cookieValue.getAttribute("value");
    alert(x);
    
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Task', 'Hours per Day'],
          ['Masculinos',     x],
          ['Femeninos',      xx]
         
        ]);

        var options = {
          title: 'Competidores por sexo',
          is3D: true,
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
        chart.draw(data, options);
      }
    </script>
